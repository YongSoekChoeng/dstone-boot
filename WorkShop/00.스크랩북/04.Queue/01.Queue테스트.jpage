/*****************************************************************/int tryNum = 100;/*****************************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("01.Queue테스트");
try {		for(int i =0; i<tryNum; i++){				net.dstone.common.queue.QueueItem item = new net.dstone.common.queue.QueueItem(){			@Override			public void doTheJob(){				System.out.println(this.getId() + "'s Job done !!");			}		};		item.setId("QueueItem-" + i);				net.dstone.common.queue.QueueHandler.getInstance().addQueue(item);	}	
} catch (Exception e) {
	e.printStackTrace();
} finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("01.Queue테스트");}