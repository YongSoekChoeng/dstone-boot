int threadNumWhenFixed = 100;
try {	net.dstone.common.utils.DateUtil.stopWatchStart("01.Task테스트");	String filePath = "D:/Temp/sampletask";		net.dstone.common.task.TaskHandler.TaskConfig conf = net.dstone.common.task.TaskHandler.getInstance().getTaskConfig();	conf.setTaskMode(net.dstone.common.task.TaskHandler.FIXED);	conf.setThreadNumWhenFixed(threadNumWhenFixed);	conf.setWaitTimeAfterShutdown(1);		java.util.ArrayList<net.dstone.common.task.TaskItem> itemList = new java.util.ArrayList<net.dstone.common.task.TaskItem>();		for(int i =0; i<100; i++){		net.dstone.common.task.TaskItem item = new net.dstone.sample.task.SampleTaskItem();		item.setId("TEST-"+ i);		item.setProperty("filePath", filePath);		item.setProperty("deleteFileYn", "Y");		itemList.add(item);	}		itemList = net.dstone.common.task.TaskHandler.getInstance().doTheTasks(conf, itemList);		for(int i =0; i<itemList.size(); i++){		net.dstone.common.task.TaskItem item = itemList.get(i);		System.out.println( "item["+item+"]" );	}	
} catch (Exception e) {
	e.printStackTrace();
} finally {	net.dstone.common.utils.DateUtil.stopWatchEnd("01.Task테스트");}