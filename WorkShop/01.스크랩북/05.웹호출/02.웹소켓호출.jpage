/*****************************************************/String url = "";String kind = "StompSockJs"; // WebSocket / StompSockJsnet.dstone.common.utils.DataSet msgDs = new net.dstone.common.utils.DataSet();if(kind.equals("WebSocket")){	url = "ws://localhost:7081/ws";	msgDs.setDatum("greetingName", "홍길동님 안녕하세요!");}else{	url = "http://localhost:7081/ws-stomp";	msgDs.setDatum("sender", "성춘향");	msgDs.setDatum("content", "뭐하냐?");}/*****************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("02.웹소켓호출");try {	if(kind.equals("WebSocket")){		net.dstone.common.utils.WsClientUtil.sendWebSocket(url, msgDs.toJson());	}else{		net.dstone.common.utils.WsClientUtil.sendStompSockJs(url, msgDs.toJson());	}} catch (Exception e) {	e.printStackTrace();} finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("02.웹소켓호출");}