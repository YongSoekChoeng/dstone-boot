/***************************************************** Global Setting Start *****************************************************//* 작업진행종류(A:소스분석, D:분석결과DB저장) */String	 				jobProcessKind		= "A";/* 진행할 작업단계(전체작업단계 JOB_KIND_11 ~ JOB_KIND_99에서 세팅된 작업단계까지 분석을 진행한다. 작업진행종류가 A:소스분석 일 경우에만 의미있음.) */int 					analyzeJobKind		= net.dstone.common.tools.analyzer.AppAnalyzer.JOB_KIND_41_ANALYZE_UI;/* 작업단계에서 단위작업만 진행할지 여부. true-해당작업(JOB_KIND_XX)만 수행. false-처음부터 해당작업까지(JOB_KIND_11 ~ JOB_KIND_XX) 수행. 작업진행종류가 A:소스분석 일 경우에만 의미있음. */boolean 				isUnitOnly			= true;/***************************************************** Global Setting End *****************************************************//* +++++++++++++++++++++++++++++++++++++++++++++ Application Setting Start +++++++++++++++++++++++++++++++++++++++++++++ */String					configPath			= "";// 프레임웍//configPath									= "D:/AppHome/framework/dstone-boot/src/main/resources/tools/analyzer/config.xml";// AIB/*configPath									= "D:/AppHome/framework/dstone-boot/src/main/resources/tools/analyzer/config.xml";*/// 동부화재/*configPath									= "D:/AppHome/framework/dstone-boot/src/main/resources/tools/analyzer/config.xml";*///토스ERPconfigPath									= "D:/AppHome/framework/dstone-boot/src/main/resources/tools/analyzer/config-anybiz.xml";//토스수수료/*configPath									= "D:/AppHome/framework/dstone-boot/src/main/resources/tools/analyzer/config-cms4_prd.xml";*//* +++++++++++++++++++++++++++++++++++++++++++++ Application Setting End +++++++++++++++++++++++++++++++++++++++++++++ */try {	net.dstone.common.utils.DateUtil.stopWatchStart("01.소스분석");	// 1.분석모듈 인스턴스 생성	net.dstone.common.tools.analyzer.AppAnalyzer appAnalyzer = net.dstone.common.tools.analyzer.AppAnalyzer.getInstance(configPath);		// 2-1. 작업진행종류 - A:소스분석	if( "A".equals(jobProcessKind) ){				// 2-1-1.초기화		if( !isUnitOnly ){			appAnalyzer.initAll();		}		// 2-1-2.분석진행(파라메터로 진행단계 조절)		appAnalyzer.analyzeApp(analyzeJobKind, isUnitOnly);			// 