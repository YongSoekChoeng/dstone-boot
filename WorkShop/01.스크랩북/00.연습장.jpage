/*****************************************************/String url = "http://localhost:8080/ocr/txt/convert-ocrtxt";String method = "POST";/*****************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("00.연습장");net.dstone.common.utils.WsUtil ws = new net.dstone.common.utils.WsUtil();try {	net.dstone.common.utils.WsUtil.Bean wsBean = new net.dstone.common.utils.WsUtil.Bean();		wsBean.url = url;	System.out.println( "wsBean.url===>>>" +  wsBean.url );	wsBean.method = method;			wsBean.setContentType(net.dstone.common.utils.WsUtil.CONT_TYPE_JSON);	//wsBean.addHeader("Content-Type", "application/json");	wsBean.addHeader("Content-Type", "application/json; charset=UTF-8");	String strBody = net.dstone.common.utils.FileUtil.readFile("C:/Temp/OCR/파싱결과/감리배치확인서.txt");	wsBean.setBody(strBody);	System.out.println( "wsBean.getBody()===>>>" +  wsBean.getBody() );		ws.execute(wsBean);	String jsonStr = ws.response.outputStr.toString();	//"UTF-8", "EUC-KR"	net.dstone.common.utils.StringUtil.encodingCheck(jsonStr);	System.out.println(jsonStr);		if( ws.response.StatusCd == 200 ){		net.dstone.common.utils.DataSet ds = new net.dstone.common.utils.DataSet();		ds.buildFromJson(jsonStr, "");		System.out.println( ds.toJson() );	}else{		System.out.println(jsonStr);	}	} catch (Exception e) {	e.printStackTrace();} finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("00.연습장");}