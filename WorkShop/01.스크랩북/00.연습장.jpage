net.dstone.common.utils.DateUtil.stopWatchStart("00.연습장");try {		java.util.List<String> paramFileList = new java.util.ArrayList<String>();	for(int i=0; i<5208; i++){		paramFileList.add(String.valueOf(i));	}		System.out.println(paramFileList.size());		System.out.println("=======================");		int totSum = 0;	java.util.List<java.util.List<String>> divFileList = null;	int chunkSize = 1;	chunkSize = Math.max((paramFileList.size()/8), 1);		divFileList = net.dstone.common.utils.PartitionUtil.ofSize(paramFileList, chunkSize);	for(java.util.List<String> divList : divFileList){		totSum = totSum + divList.size();	}	System.out.println(totSum);	} catch (Exception e) {	e.printStackTrace();}finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("00.연습장");}