/*****************************************************/String path = "D:/Temp/TestSql.xml";/*****************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("00.연습장");try {	net.dstone.common.utils.XmlUtil xml = net.dstone.common.utils.XmlUtil.getInstance(net.dstone.common.utils.XmlUtil.XML_SOURCE_KIND_PATH, path);		org.w3c.dom.Node node = xml.getNodeByExp("//select[@id='selectMappingTotalCnt']");		//System.out.println("org.w3c.dom.Node.ELEMENT_NODE:" + org.w3c.dom.Node.ELEMENT_NODE);	//System.out.println("org.w3c.dom.Node.ELEMENT_NODE:" + org.w3c.dom.Node.);	//System.out.println(node);	org.w3c.dom.NodeList nodeList = node.getChildNodes();	for(int i=0; i< nodeList.getLength(); i++ ){		org.w3c.dom.Node cNode = nodeList.item(i);		if( org.w3c.dom.Node.ELEMENT_NODE == cNode.getNodeType() ){			node.removeChild(cNode);		}	}	System.out.println(node.getTextContent());	} catch (Exception e) {	e.printStackTrace();}finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("00.연습장");}