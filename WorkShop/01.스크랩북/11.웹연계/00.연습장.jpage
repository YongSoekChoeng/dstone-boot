net.dstone.common.utils.DataSet paramDs = new net.dstone.common.utils.DataSet();/*****************************************************/String url = "";url = "https://maps.googleapis.com/maps/api/distancematrix/json";// 37.504431, 127.024647paramDs.addDatum("origins"		, "37.5665,126.9780");paramDs.addDatum("destinations"	, "37.5796,126.9770");paramDs.addDatum("mode"	, "walking");  // driving, walking, transit, bicyclingparamDs.addDatum("key"	, "AIzaSyDu-nIXG2yLg-Y2HgiRWKuupadjMXjnRT8");/*****************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("00.연습장");net.dstone.common.utils.WcUtil ws = new net.dstone.common.utils.WcUtil();try {	net.dstone.common.utils.WcUtil.Bean wsBean = new net.dstone.common.utils.WcUtil.Bean();	String method = "GET";	if( "GET".equals(method) ){		String[] paramKeys = paramDs.getChildrenKeys();		if(paramKeys != null){			for(String key : paramKeys){				if(url.indexOf("?") < 0){					url = url + "?";				}else{					url = url + "&";				}				url = url + key + "=" + paramDs.getDatum(key);			}		}	}else{		String[] paramKeys = paramDs.getChildrenKeys();		if(paramKeys != null){			for(String key : paramKeys){				wsBean.addParam(key, paramDs.getDatum(key));			}		}	}		wsBean.url = url;	System.out.println( "wsBean.url===>>>" +  wsBean.url );	wsBean.method = method;			wsBean.setContentType(net.dstone.common.utils.WcUtil.CONT_TYPE_JSON);	wsBean.addHeader("Content-Type", "application/json; charset=UTF-8");	ws.execute(wsBean);	String jsonStr = ws.response.outputStr.toString();	System.out.println(jsonStr);		if( ws.response.StatusCd == 200 ){		net.dstone.common.utils.DataSet ds = new net.dstone.common.utils.DataSet();		ds.buildFromJson(jsonStr, "");		System.out.println( ds.toJson() );	}else{		System.out.println(jsonStr);	}	} catch (Exception e) {	e.printStackTrace();} finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("00.연습장");}