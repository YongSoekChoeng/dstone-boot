/*****************************************************/String url = "";String method = "GET";net.dstone.common.utils.DataSet paramDs = new net.dstone.common.utils.DataSet();String placeName = "";String mode = "주소목록";	// 주소목록/주소상세/* 주소목록 */if("주소목록".equals(mode)){	url = "https://openapi.naver.com/v1/search/local.json";	placeName = "서울특별시 강서구 곰달래로";/* 주소상세 */}else{	url = "https://maps.apigw.ntruss.com/map-geocode/v2/geocode";	placeName = "서울특별시 강서구 화곡동 1159-4";}placeName = java.net.URLEncoder.encode(placeName, "UTF-8");method = "GET";paramDs.clear();paramDs.setDatum("query", placeName);paramDs.setDatum("start", "1");paramDs.setDatum("display", "100");/*****************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("03.NAVER주소검색");net.dstone.common.utils.WsUtil ws = new net.dstone.common.utils.WsUtil();try {	net.dstone.common.utils.WsUtil.Bean wsBean = new net.dstone.common.utils.WsUtil.Bean();		if( "GET".equals(method) ){		String[] paramKeys = paramDs.getChildrenKeys();		if(paramKeys != null){			for(String key : paramKeys){				if(url.indexOf("?") < 0){					url = url + "?";				}else{					url = url + "&";				}				url = url + key + "=" + paramDs.getDatum(key);			}		}	}else{		String[] paramKeys = paramDs.getChildrenKeys();		if(paramKeys != null){			for(String key : paramKeys){				wsBean.addParam(key, paramDs.getDatum(key));			}		}	}	wsBean.url = url;	System.out.println( "wsBean.url===>>>" +  wsBean.url );	wsBean.method = method;			wsBean.setContentType(net.dstone.common.utils.WsUtil.CONT_TYPE_JSON);	/* 주소목록 */	if("주소목록".equals(mode)){		wsBean.addHeader("X-Naver-Client-Id", "OVGBzirqfGgX3WpQziwx");		wsBean.addHeader("X-Naver-Client-Secret", "HhXh0R1Oum" );	/* 주소상세 */	}else{		wsBean.addHeader("x-ncp-apigw-api-key-id", "x2g1bf9qul");		wsBean.addHeader("x-ncp-apigw-api-key", "pjwo8bTSJuUR4nLxyDS79mJ7Y0HM3xmaYRp7ZSjJ" );	}	if( "POST".equals(method) ){		wsBean.setBody(paramDs.toJson());		System.out.println( "wsBean.getBody()===>>>" +  wsBean.getBody() );	}		ws.execute(wsBean);	String jsonStr = ws.response.outputStr.toString();	//System.out.println(jsonStr);	if( ws.response.StatusCd == 200 ){		net.dstone.common.utils.DataSet ds = new net.dstone.common.utils.DataSet();		ds.buildFromJson(jsonStr, "");		System.out.println( ds.toJson() );	}else{		System.out.println(jsonStr);	}	} catch (Exception e) {	e.printStackTrace();} finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("03.NAVER주소검색");}