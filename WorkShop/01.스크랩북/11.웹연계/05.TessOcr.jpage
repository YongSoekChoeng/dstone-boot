/*****************************************************/String pdfPath = "C:\WorkShop/프로젝트/03.개발/01.OCR/eformsign_invoice_20250401.pdf";pdfPath = "C:\WorkShop/프로젝트/03.개발/01.OCR/OCR 문서 사본/이행(선금)보증보험.pdf";pdfPath = "C:\WorkShop/프로젝트/03.개발/01.OCR/OCR 양수도 문서 사본/7. 사업자등록증_햇살그린협동조합_240717.pdf";/*****************************************************/net.dstone.common.utils.DateUtil.stopWatchStart("05.Tess Ocr");org.springframework.web.client.RestTemplate rTemplete = null;org.springframework.http.HttpHeaders headers = null;org.springframework.http.HttpEntity<String> entity = null;try {	// PDF => JPG로 변환	String pagesPath = net.dstone.common.utils.FileUtil.getFilePath(pdfPath) + "/" +  net.dstone.common.utils.FileUtil.getFileName(pdfPath, false) + "/tess";	net.dstone.common.utils.FileUtil.deleteDir(pagesPath);	net.dstone.common.utils.FileUtil.convertPdfToJpg(pdfPath, pagesPath);		net.sourceforge.tess4j.Tesseract tesseract = null;		String[] pages = net.dstone.common.utils.FileUtil.readFileListAll(pagesPath);	int pageNum = 0;	for(String page : pages){		if(pageNum>0){break;}		tesseract = new net.sourceforge.tess4j.Tesseract();		        // Tesseract 설치 경로 설정        tesseract.setDatapath("C:/Program Files/Tesseract-OCR/tessdata"); // 디렉토리 경로        tesseract.setLanguage("kor+eng"); // 한글 + 영어                try {            String result = tesseract.doOCR(new java.io.File(page));            //String title = extractTitle(result);            System.out.println("result: " + result);                        net.dstone.common.utils.FileUtil.writeFile(pagesPath, net.dstone.common.utils.FileUtil.getFileName(page, false)+".txt", result  );        } catch (Exception e) {            e.printStackTrace();        }        pageNum++;	}	} catch (Exception e) {	e.printStackTrace();} finally{	net.dstone.common.utils.DateUtil.stopWatchEnd("05.Tess Ocr");}